{
  "name": "lmwebui",
  "version": "1.0.0",
  "description": "LMWebUI - A modern, privacy-focused Language Model Web UI for local Ollama models. Run language models locally with a beautiful web interface.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "test:coverage": "echo \"No tests specified\" && exit 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "husky install"
  },
  "keywords": [
    "lmwebui",
    "language-model",
    "web-ui",
    "ollama",
    "ai",
    "chat",
    "privacy",
    "local",
    "open-source",
    "react",
    "typescript",
    "tailwindcss",
    "chatgpt",
    "llm",
    "machine-learning",
    "web-interface",
    "local-ai"
  ],
  "author": {
    "name": "LMWebUI Team",
    "email": "team@lmwebui.com",
    "url": "https://github.com/ismailcankaratas/LMWebUI"
  },
  "license": "MIT",
  "homepage": "https://lmwebui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ismailcankaratas/LMWebUI.git"
  },
  "bugs": {
    "url": "https://github.com/ismailcankaratas/LMWebUI/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.7.3",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
